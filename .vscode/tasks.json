{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: g++ build active file",
      "command": "/usr/bin/g++",
      "args": [
        "-std=c++14",
        "-g",
        "${workspaceFolder}/VectorSorting.cpp",
        "${workspaceFolder}/CSVparser.cpp",
        "-o",
        "${workspaceFolder}/VectorSorting"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Generated task by Debugger."
    },
    {
      "label": "Run VectorSorting",
      "type": "shell",
      "command": "${workspaceFolder}/VectorSorting",
      "args": [
        "eBid_Monthly_Sales.csv"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": {
        "kind": "test",
        "isDefault": true
      },
      "problemMatcher": []
    }
  ],
  "version": "2.0.0"
}